{
  "name": "skeleton",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "release:local": "env-cmd -f .env.local next dev -- -p 3000",
    "release:production": "env-cmd -f .env.production next dev -- -p 3000",
    "build:local": "env-cmd -f .env.local next build && next start -- -p 3000",
    "build:production": "env-cmd -f .env.production next build && next start -- -p 3000"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "date-fns": "^2.22.1",
    "env-cmd": "^10.1.0",
    "lodash": "^4.17.21",
    "next": "11.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/react": "17.0.13",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "eslint": "^7.29.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-next": "11.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.3.2",
    "typescript": "4.3.5"
  }
}
